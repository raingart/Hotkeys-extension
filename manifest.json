{
   "manifest_version": 3,
   "name": "Hotkeys",
   "version": "0.2",
   "description": "__MSG_app_description__",
   "default_locale": "en",
   "minimum_chrome_version": "88",
   "action": {
      "default_popup": "/html/options.html"
   },
   "options_page": "/html/options.html",
   "background": {
      "service_worker": "/js/background.js"
   },
   "permissions": [
      "storage"
   ],
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "/js/libs/browser-support.js",
            "/js/libs/storage.js",
            "/js/libs/shortcut.js",
            "/js/context.js"
         ],
         "run_at": "document_start"
      }
   ]
}
